{
  "manifest_version": 2,

  "name": "Foros Del Web Skin",
  "description": "Skin para forosdelweb.com",
  "version": "1.0.4",

  "content_scripts": [{
    "matches": ["http://www.forosdelweb.com/*"],
    "css": ["data/css/global.css","data/css/posts.css","data/css/blackened.css"],
    "js": ["data/js/jquery.min.js", "data/js/fdwskin.js"]
    }],

  "web_accessible_resources": [
       "data/images/*.png"
  	],

  "icons": {
    "128": "data/icons/fdw.png"
  	}
}
